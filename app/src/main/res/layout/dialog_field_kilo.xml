<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="300dp"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:layout_editor_absoluteY="25dp">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="78dp"
        android:layout_marginLeft="17dp"
        android:layout_marginRight="78dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="80dp"
        android:text="К"
        app:layout_constraintEnd_toStartOf="@+id/editK"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="95dp"
        android:layout_marginEnd="77dp"
        android:layout_marginLeft="17dp"
        android:layout_marginRight="77dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="95dp"
        android:text="И"
        app:layout_constraintBottom_toBottomOf="@+id/editL"
        app:layout_constraintEnd_toStartOf="@+id/editI"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="78dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="78dp"
        android:layout_marginStart="16dp"
        android:text="Л"
        app:layout_constraintBaseline_toBaselineOf="@+id/editL"
        app:layout_constraintEnd_toStartOf="@+id/editL"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editK"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="68dp"
        android:ems="10"
        android:hint="ЧТО?"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editI"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="148dp"
        android:ems="10"
        android:hint="СКОЛЬКО?"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editL"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="38dp"
        android:ems="10"
        android:hint="ГДЕ?"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/editI" />

    <EditText
        android:id="@+id/editO"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="37dp"
        android:ems="10"
        android:hint="КОГДА?"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="@+id/editL"
        app:layout_constraintTop_toBottomOf="@+id/editL" />

    <Button
        android:id="@+id/good_but"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="32dp"
        android:text="Заполнить"
        app:layout_constraintStart_toStartOf="@+id/editO"
        app:layout_constraintTop_toBottomOf="@+id/editO" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="76dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="76dp"
        android:layout_marginStart="16dp"
        android:text="О"
        app:layout_constraintBaseline_toBaselineOf="@+id/editO"
        app:layout_constraintEnd_toStartOf="@+id/editO"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="404dp"
        android:text="Пояснения КИЛО"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.556"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>